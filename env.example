# Clerk Authentication
# Get your publishable key from https://dashboard.clerk.com/
REACT_APP_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key_here

# Optional: Stripe (for payments)
# REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Stripe Configuration (REQUIRED)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# Stripe Price IDs (REQUIRED - create these in your Stripe dashboard)
REACT_APP_STRIPE_PRO_PRICE_ID=price_pro_monthly_id_here
REACT_APP_STRIPE_ENTERPRISE_PRICE_ID=price_enterprise_monthly_id_here

# Frontend Environment Variables (REQUIRED)
REACT_APP_API_URL=https://your-domain.vercel.app

# Server Configuration
PORT=3001

# Production Configuration
NODE_ENV=production
FRONTEND_URL=https://your-domain.vercel.app

# Development Test Mode (OPTIONAL)
REACT_APP_ENABLE_TEST_MODE=false 